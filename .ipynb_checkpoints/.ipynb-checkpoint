    "#Sum the profits for the companies\n",
    "df_clean = df_combined.groupby('Company').agg({'profits':['sum']})\n",
    "df_clean.columns = ['profits_sum']\n",
    "df_clean.sort_values(by='profits_sum', ascending=False)\n",
    "top_20 = df_clean.sort_values(by='profits_sum', ascending=False)[:20].sum()\n",
    "total = df_clean.sort_values(by='profits_sum', ascending=False).sum()\n",
    "df_clean.sort_values(by='profits_sum', ascending=False)"
